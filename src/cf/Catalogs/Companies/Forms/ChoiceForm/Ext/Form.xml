	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.OurCompany</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Table.Ref,
	Table.DeletionMark,
	Table.OurCompany,
	Table.Partner,
	Table.Description_en AS Description,
	Table.Code
FROM
	Catalog.Companies AS Table
WHERE
	CASE
		WHEN &amp;FilterByPartnerHierarchy
			THEN Table.Ref IN (&amp;CompaniesByPartnerHierarchy)
		ELSE TRUE
	END</QueryText>
