				<QueryText>SELECT
	InventoryTransferItemList.Ref AS Ref,
	SUM(ISNULL(InventoryTransferItemList.QuantityInBaseUnit, 0)) AS QuantityInBaseUnit
INTO ItemListTable
FROM
	Document.InventoryTransfer.ItemList AS InventoryTransferItemList
GROUP BY
	InventoryTransferItemList.Ref

INDEX BY
	Ref
;

////////////////////////////////////////////////////////////////////////////////
 SELECT
	CASE
		WHEN ISNULL(R4032B_GoodsInTransitOutgoingBalance.QuantityBalance, 0) = 0
			THEN VALUE(Enum.ShippingStatuses.Closed)
		WHEN ItemListTable.QuantityInBaseUnit = ISNULL(R4032B_GoodsInTransitOutgoingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Awaiting)
		WHEN ItemListTable.QuantityInBaseUnit &lt;&gt; ISNULL(R4032B_GoodsInTransitOutgoingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Shipping)
	END AS StatusOutgoing,
	CASE
		WHEN ISNULL(R4031B_GoodsInTransitIncomingBalance.QuantityBalance, 0) = 0
			THEN VALUE(Enum.ShippingStatuses.Closed)
		WHEN ItemListTable.QuantityInBaseUnit = ISNULL(R4031B_GoodsInTransitIncomingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Awaiting)
		WHEN ItemListTable.QuantityInBaseUnit &lt;&gt; ISNULL(R4031B_GoodsInTransitIncomingBalance.QuantityBalance, 0)
			THEN VALUE(Enum.ShippingStatuses.Shipping)
	END AS StatusIncoming,
	InventoryTransfer.Ref,
	InventoryTransfer.Number,
	InventoryTransfer.Date,
	InventoryTransfer.Company,
	InventoryTransfer.StoreReceiver,
	InventoryTransfer.StoreSender,
	InventoryTransfer.StoreTransit,
	InventoryTransfer.Author
FROM
	Document.InventoryTransfer AS InventoryTransfer
		LEFT JOIN ItemListTable AS ItemListTable
		ON InventoryTransfer.Ref = ItemListTable.Ref
		LEFT JOIN AccumulationRegister.R4032B_GoodsInTransitOutgoing.Balance AS R4032B_GoodsInTransitOutgoingBalance
		ON InventoryTransfer.Ref = CAST(R4032B_GoodsInTransitOutgoingBalance.Basis AS Document.InventoryTransfer)
		LEFT JOIN AccumulationRegister.R4031B_GoodsInTransitIncoming.Balance AS R4031B_GoodsInTransitIncomingBalance
		ON InventoryTransfer.Ref = CAST(R4031B_GoodsInTransitIncomingBalance.Basis AS Document.InventoryTransfer)</QueryText>
