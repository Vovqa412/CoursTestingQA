	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	SalesOrderList.Ref,
	SalesOrderList.DeletionMark,
	SalesOrderList.Number,
	SalesOrderList.Date,
	SalesOrderList.Posted,
	SalesOrderList.Partner,
	SalesOrderList.Company,
	SalesOrderList.Currency,
	SalesOrderList.DateOfShipment,
	SalesOrderList.ManagerSegment,
	SalesOrderList.Status,
	SalesOrderList.Agreement,
	SalesOrderList.PriceIncludeTax,
	SalesOrderList.DocumentAmount,
	SalesOrderList.Author,
	Not SalesOrderClosing.Ref IS NULL AS Closed,
	SalesOrderClosing.Ref AS ClosingOrder,
	CASE WHEN COUNT(DISTINCT SalesOrderItemList.Store) = 1 THEN
		MAX(SalesOrderItemList.Store)
	ELSE UNDEFINED END AS Store
FROM
	Document.SalesOrder AS SalesOrderList
		LEFT JOIN Document.SalesOrderClosing AS SalesOrderClosing
		ON SalesOrderClosing.SalesOrder = SalesOrderList.Ref
		AND SalesOrderClosing.Posted
		AND SalesOrderClosing.CloseOrder
		LEFT JOIN Document.SalesOrder.ItemList AS SalesOrderItemList
		ON SalesOrderItemList.Ref = SalesOrderList.Ref
GROUP BY
	SalesOrderList.Ref,
	SalesOrderList.DeletionMark,
	SalesOrderList.Number,
	SalesOrderList.Date,
	SalesOrderList.Posted,
	SalesOrderList.Partner,
	SalesOrderList.Company,
	SalesOrderList.Currency,
	SalesOrderList.DateOfShipment,
	SalesOrderList.ManagerSegment,
	SalesOrderList.Status,
	SalesOrderList.Agreement,
	SalesOrderList.PriceIncludeTax,
	SalesOrderList.DocumentAmount,
	SalesOrderList.Author,
	NOT SalesOrderClosing.Ref IS NULL,
	SalesOrderClosing.Ref</QueryText>
				<MainTable>Document.SalesOrder</MainTable>
